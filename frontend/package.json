{
  "name": "glitter-graphix",
  "version": "0.0.1",
  "main": "dist/index.js",
  "author": "Viihna Lehraine",
  "license": "GNU GPLv3",
  "type": "module",
  "scripts": {
    "build": "pnpm run prebuild && pnpm run build:assets && vite build",
    "build:assets": "pnpm exec tsx src/app/build/gen_asset_manifest.ts",
    "clean": "rm -rf node_modules dist",
    "dev": "vite",
    "format": "prettier --write \"**/*.{ts,js,json,css,md,html}\"",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --fix --ext .ts src/",
    "prebuild": "rm -rf public/assets && mkdir -p public && cp -r ../assets public",
    "preview": "vite preview",
    "reinit": "pnpm clean && pnpm install",
    "test": "vitest run",
    "test:full": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "watch": "pnpm watch:ts & pnpm watch:test",
    "watch:test": "vitest --watch",
    "watch:ts": "tsc --watch"
  },
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@types/gif.js": "^0.2.5",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "image-size": "^2.0.2",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "browserify": "^17.0.1",
    "gif.js": "^0.2.0",
    "gifuct-js": "^2.1.2",
    "html2canvas": "^1.4.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.8.1"
  }
}
