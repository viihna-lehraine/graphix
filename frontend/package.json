{
  "name": "glitter-graphix",
  "version": "0.0.1",
  "main": "dist/index.js",
  "author": "Viihna Lehraine",
  "license": "GNU GPLv3",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && vite build && mkdir -p ./dist/static && cp -r ./static/* ./dist/static/",
    "clean": "rm -rf node_modules dist && pnpm cache clean*",
    "dev": "vite",
    "format": "prettier --write \"**/*.{ts,js,json,css,md,html}\"",
    "lint": "eslint --ext .ts ts/",
    "lint:fix": "eslint --fix --exit .ts ts/",
    "reinit": "rm -rf node_modules && pnpm install",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch"
  },
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.8.1"
  }
}
